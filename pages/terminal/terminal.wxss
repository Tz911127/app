@import "../../lib/lib.wxss";

page {
  background-color: #edf0f3;
}

.page {
  /* background-color: rgba(242, 242, 242, 1); */
  position: absolute;
  height: 100%;
  width: 100%;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.page-section {
  /* margin-bottom: 20rpx; *//* position: relative; *//* top: 0; */
  width: 100%;
}

.page-tab {
  height: 100%;
  width: 70%;
  position: absolute;
  z-index: 10;
  background: #fff;
  right: 0;
}

.home {
  position: absolute;
  width: 750rpx;
  height: 100%;
  background-color: rgba(242, 242, 242, 1);
  z-index: 12;
  transition: All 0.4s ease;
  -webkit-transition: All 0.4s ease;
}

/* 遮罩层 */

.bg {
  display: none;
  position: fixed;
  top: 0%;
  right: 70%;
  width: 100%;
  height: 100%;
  background-color: black;
  z-index: 1001;
  -moz-opacity: 0.7;
  opacity: 0.70;
  transition: width 2s;
  filter: alpha(opacity=70);
}

.status_line {
  background-repeat: no-repeat;
  background-position: 0rpx 11rpx;
  display: inline-block;
  text-align: center;
  padding-top: 20rpx;
  color: #98a0b3;
  width: 180rpx;
  padding-bottom: 20rpx;
  border-radius: 20rpx;
  margin: 0 20rpx;
  font-size: 24rpx;
}

.scroll-view_H {
  white-space: nowrap;
}

.online {
  background-image: url('data:image/png;base64,');
}

.online:hover {
  color: #fff;
  background-color: #096dd9;
  background-image: url('data:image/png;base64,');
}

.offline {
  background-image: url('data:image/png;base64,');
}

.offline:hover {
  color: #fff;
  background-color: #096dd9;
  background-image: url('data:image/png;base64,');
}

.error {
  background-image: url('data:image/png;base64,');
}

.error:hover {
  color: #fff;
  background-color: #096dd9;
  background-image: url('data:image/png;base64,');
}

.timeout {
  background-image: url('data:image/png;base64,');
}

.timeout:hover {
  color: #fff;
  background-color: #096dd9;
  background-image: url('data:image/png;base64,');
}

.btn {
  display: inline-block;
  border: 1px solid rgba(228, 228, 228, 1);
  border-radius: 57rpx;
  width: 83%;
  background-color: rgba(242, 242, 242, 1);
  height: 70rpx;
}

