/* pages/sel_tem/sel_tem.wxss */
@import "../../lib/lib.wxss";

.test {
  flex: 90%;
}

page {
  background-color: #edf0f3;
}

.test .tit {
  color: #2b344a;
  font-size: 32rpx;
}

.test .main {
  color: #98a0b3;
  position: relative;
}

.test image {
  width: 40rpx;
  height: 40rpx;
}

.weui-cell__hd {
  line-height: 150rpx;
  margin-right: 20rpx;
}

.stu {
  background: #096dd9;
  font-size: 25rpx;
  display: inline-block;
  color: #fff;
  width: 80rpx;
  text-align: center;
  border-radius: 25rpx;
}

.scroll_view {
  height: 100vh;
  margin-top: 83rpx;
}

.checkbox {
  margin-right: 20rpx;
}

.lab {
  background: #fff;
}

.ok_btn {
  position: fixed;
  bottom: 0;
  background: #fff;
  z-index: 2;
  width: 100%;
  border-top: 1px solid #f5f7fb;
  height: 130rpx;
  line-height: 130rpx;
  padding-left: 30rpx;
}

.ok_btn label, .ok_btn view {
  display: inline-block;
}

.ok_btn .ok_pub {
  float: right;
  background: linear-gradient(to bottom right, #40a9ff, #096dd9);
  color: #fff;
  padding: 0 60rpx;
  margin-right: 32rpx;
}

.ok_btn .ok_rad {
  width: 55%;
  position: relative;
}

.ok_total {
  float: right;
}

.red {
  background: #ff4d4f;
}

.blue {
  background: #096dd9;
}

.green {
  background: #98a0b3;
}

.yellow {
  background: #fa8c16;
}

.page-tab {
  height: 100%;
  width: 70%;
  position: absolute;
  z-index: 10;
  background: #fff;
  right: 0;
}

.page-tab .btn {
  width: 30%;
  display: inline-block;
  margin-right: 10rpx;
}

.page-tab .tab {
  margin: 30rpx;
}

.page-tab .tab view {
  font-size: 33rpx;
  font-family: 400;
}

.status_line {
  background-repeat: no-repeat;
  background-position: 0rpx 11rpx;
  display: inline-block;
  text-align: center;
  padding-top: 20rpx;
  color: #98a0b3;
  width: 180rpx;
  padding-bottom: 20rpx;
  border-radius: 20rpx;
  margin: 0 20rpx;
  font-size: 24rpx;
}

.online {
  background-image: url('data:image/png;base64,');
}

.offline {
  background-image: url('data:image/png;base64,');
}

.error {
  background-image: url('data:image/png;base64,');
}

.timeout {
  background-image: url('data:image/png;base64,');
}

.btn_ {
  display: inline-block;
  text-align: center;
  justify-content: center;
  width: 45%;
  margin: 0 10rpx;
  font-size: 25rpx;
  border-radius: 15rpx;
}

.turn {
  padding: 25rpx 0;
  font-size: 30rpx;
}

.turn image {
  width: 8rpx;
  height: 14rpx;
}

.ok_off {
  display: flex;
  flex-direction: row;
  text-align: center;
}

.ok_off view {
  width: 45%;
  font-size: 25rpx;
  border-radius: 8rpx;
}

.ok_off .sub_btn {
  background: linear-gradient(to bottom right, #40a9ff, #096dd9);
  color: #fff;
  margin-left: 20rpx;
  /* padding: 14rpx; */
  font-size: 28rpx;
  width: 59%
}

.bg {
  display: none;
  position: fixed;
  top: 0%;
  right: 70%;
  width: 100%;
  height: 100%;
  background-color: black;
  z-index: 1001;
  -moz-opacity: 0.7;
  opacity: 0.70;
  transition: width 2s;
  filter: alpha(opacity=70);
}

.sea {
  display: inline-block;
  padding-left: 30rpx;
  position: absolute;
  top: 4rpx;
}

.sea image {
  width: 56rpx;
  height: 56rpx;
}

.sear {
  position: fixed;
  width: 100%;
  z-index: 333;
  background: #fff;
  padding-bottom: 15rpx;
  padding-left: 20rpx;
  box-shadow: 0rpx 3rpx 4rpx #f5f7f8;
}

.btn {
  display: inline-block;
  /* border: 1px solid rgba(228, 228, 228, 1); */
  border-radius: 57rpx;
  width: 80%;
  background-color: #f5f7f8;
  height: 60rpx;
}

.page-tab .btn {
  width: 30%;
  display: inline-block;
  margin-right: 10rpx;
}

.nav {
  display: flex;
  background: #fff;
  padding-left: 30rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  margin: 10rpx 20rpx 16rpx 20rpx;
  position: relative;
  font-size: 28rpx;
}

.btn image {
  width: 44rpx;
  height: 44rpx;
  margin-right: 20rpx;
  margin-left: 20rpx;
  margin-top: 8rpx;
}

.btn input {
  display: inline-block;
  width: 70%;
  position: absolute;
  top: 6rpx;
  font-size: 32rpx;
}

.page-tab {
  height: 100%;
  width: 70%;
  position: absolute;
  z-index: 10;
  background: #fff;
  right: 0;
}

.home {
  position: absolute;
  width: 750rpx;
  height: 100%;
  background-color: #f5f7f8;
  z-index: 12;
  transition: All 0.4s ease;
  -webkit-transition: All 0.4s ease;
}

.select_none {
  width: 40rpx;
  height: 40rpx;
  border: 1rpx solid #d6d6d6;
  border-radius: 50%;
  position: absolute;
  top: 45rpx;
}

.icon {
  position: absolute;
  top: 25rpx;
}

checkbox .wx-checkbox-input {
  border-radius: 50%;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  height: 65rpx;
  width: 65rpx;
  line-height: 65rpx;
  text-align: center;
  background-color: #096dd9;
  /* border: 1rpx solid #096dd9; */
  border-radius: 50%;
  color: #fff;
  /* 自定义样式.... */
}
.btn_city {
 display: inline-block;
  text-align: center;
  justify-content: center;
  width: 45%;
  margin: 0 10rpx;
  font-size: 25rpx;
  border-radius: 8rpx;
  background: #fff;
  box-shadow: 0rpx 8rpx 16rpx #f5f7f8;
}