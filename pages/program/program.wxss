@import "../../lib/lib.wxss";

page {
  background-color: #f5f7f8;
}

.page {
  position: absolute;
  /* height: 100%; */
  width: 100%;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.page-section {
  width: 100%;
}

.page-tab {
  /* height: 100%; */
  width: 70%;
  position: absolute;
  z-index: 10;
  background: #fff;
  right: 0;
}

.home {
  position: absolute;
  width: 750rpx;
  background-color: #f5f7f8;
  z-index: 12;
  transition: All 0.4s ease;
  -webkit-transition: All 0.4s ease;
}

/* 遮罩层 */

.bg {
  display: none;
  position: fixed;
  top: 0%;
  right: 70%;
  width: 100%;
  height: 100%;
  background-color: black;
  z-index: 1001;
  -moz-opacity: 0.7;
  opacity: 0.70;
  transition: width 2s;
  filter: alpha(opacity=70);
}

input {
  width: 100%;
  padding-left: 20rpx;
}

.nav {
  display: flex;
  background: #fff;
  margin-top: 20rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
}

.img {
  flex: 10%;
}

.img image {
  width: 180rpx;
  height: 180rpx;
}

.test {
  flex: 45%;
}

.test .tit {
  font-size: 30rpx;
}

.test text {
  font-size: 27rpx;
}

.num {
  text-align: right;
  font-size: 28rpx;
  color: #98a0b3;
  margin: 15rpx 0;
}
.sta {
  text-align: left;
  font-size: 28rpx;
  color: #2b344a;
  margin: 15rpx 0;
}

.main {
  background: #fff;
  padding-bottom: 15rpx;
  margin-bottom: 16rpx;
}

.btn_ {
  display: inline-block;
  text-align: center;
  justify-content: center;
  width: 45%;
  margin: 0 10rpx;
  font-size: 25rpx;
  border-radius: 8rpx;
  background: #fff;
  box-shadow: 0rpx 8rpx 16rpx #f5f7f8;
  padding: 10rpx 0;
}

.btn_city {
 display: inline-block;
  text-align: center;
  justify-content: center;
  /* width: 45%; */
  /* margin: 0 10rpx; */
  font-size: 25rpx;
  border-radius: 8rpx;
  background: #fff;
  box-shadow: 0rpx 8rpx 16rpx #f5f7f8;
  padding: 0 20rpx;
  width:170rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  margin-left: 10rpx;

}

.btn {
  display: inline-block;
  border-radius: 57rpx;
  width: 80%;
  background-color: #f5f7f8;
  height: 60rpx;
}

.status_line {
  background-repeat: no-repeat;
  background-position: 75rpx 11rpx;
  display: inline-block;
  text-align: center;
  padding-top: 60rpx;
  color: #98a0b3;
  width: 210rpx;
  padding-bottom: 20rpx;
  border-radius: 8rpx;
  margin: 0 10rpx;
  font-size: 24rpx;
  box-shadow:0rpx 8rpx 16rpx #f5f7f8;

}

.online {
  background-image: url('data:image/png;base64,');
}

.offline {
  background-image: url('data:image/png;base64,');
}

.inline {
  background-image: url('data:image/png;base64,');
}

.okline {
  background-image: url('data:image/png;base64,');
}

.page-tab .tab {
  height: 96vh;
  margin: 0 30rpx;
}

.page-tab .tab view {
  font-size: 27rpx;
  font-family: 400;
}

.page-tab .tab button {
  font-size: 25rpx;
  padding: 0 8rpx;
}

.turn {
  border-top: 1px solid rgba(242, 242, 242, 1);
  border-bottom: 1px solid rgba(242, 242, 242, 1);
  margin: 60rpx 0;
  padding: 25rpx 0;
}

.turn image {
  width: 8rpx;
  height: 14rpx;
}

.ok_off {
  display: flex;
  flex-direction: row;
  text-align: center;
  margin-top: 40rpx;
    position: fixed;
  bottom: 0;
  width: 62%;
  margin-bottom: 20rpx
}

.ok_off view {
  width: 45%;
  font-size: 25rpx;
  border-radius: 8rpx;
}

.ok_btn {
  background: linear-gradient(to bottom right, #40a9ff, #096dd9);
  color: #fff;
  margin-left: 20rpx;
  padding: 14rpx;
  width: 59%;
}

.foot {
  border-top: 1px solid rgba(242, 242, 242, 1);
  display: flex;
  margin: 10rpx;
  padding-top: 25rpx;
}

.foot view {
  width:100%;
  font-size: 32rpx;
  text-align: center;
}

.foot view:nth-child(1) {
  color: #096dd9;
}

.foot view:nth-child(2) {
  color: #fa8c16;
}

.foot view:nth-child(3) {
  color: #fa8c16;
}

.sear {
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 333;
  background: #fff;
  padding-bottom: 15rpx;
  padding-left: 20rpx;
  box-shadow: 0rpx 3rpx 4rpx #f5f7f8;
}

.sea {
  display: inline-block;
  padding-left: 30rpx;
  padding-right: 30rpx;
  position: absolute;
  top: -6rpx;
}

.sea image {
  width: 56rpx;
  height: 56rpx;
}

.btn image {
  width: 44rpx;
  height: 44rpx;
  margin-right: 20rpx;
  margin-left: 20rpx;
  margin-top: 8rpx;
}

.btn input {
  display: inline-block;
  width: 70%;
  position: absolute;
  top: 6px;
  font-size: 26rpx;
}

.title {
  /* width: 100%; */
  /* padding-left: 35rpx; */
  padding: 20rpx 35rpx;
  border-bottom: 1px solid rgba(228, 228, 228, 1);
  font-weight: 600
}

.title view {
  font-size:32rpx;
  display: inline-block;
}

.title view:nth-child(1) {
  color: #2b344a;
}
